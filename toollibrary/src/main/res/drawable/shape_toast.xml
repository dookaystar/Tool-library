<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- 圆角 -->
    <corners android:radius="5dp" /><!-- 设置圆角半径 -->

    <!-- 间隔 -->
    <padding
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp" /><!-- 各方向的间隔 -->

    <!-- 填充 -->
    <solid android:color="@color/colorsix" /><!-- 填充的颜色 -->

    <!--描边-->
    <stroke
        android:width="0.5dp"
        android:color="#393939" />
</shape>